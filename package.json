{
  "name": "n8n-nodes-supermetrics",
  "version": "0.1.12",
  "description": "Supermetrics node for n8n",
  "homepage": "https://github.com/supermetrics-public/n8n_connector",
  "author": {
    "name": "Supermetrics",
    "email": "dev-info@supermetrics.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supermetrics-public/n8n_connector.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "n8n-community-node",
    "n8n-community-node-package",
    "n8n-nodes",
    "supermetrics"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts,package.json",
    "lintfix": "eslint . --ext .ts,package.json --fix",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm install --package-lock-only"
    }
  },
  "peerDependencies": {
    "n8n-core": ">=1.100.0",
    "n8n-workflow": ">=1.100.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "husky": "^9.1.7",
    "n8n-workflow": ">=1.100.0",
    "typescript": "^5.9.2"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Supermetrics/Supermetrics.node.js"
    ],
    "credentials": [
      "dist/credentials/SupermetricsApi.credentials.js"
    ]
  }
}
